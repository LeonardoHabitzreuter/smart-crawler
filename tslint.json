{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "only-arrow-functions": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-any": true,
    "no-console": false,
    "max-classes-per-file": false,
    "semicolon": [
      true,
      "never"
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}